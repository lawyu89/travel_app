<div class="total-page-container">
  <header class="debug">
    <div class="debug clearfix">
      <div class="debug fl-l p-l-sm">
        <% if current_user %>
          <a href='/logout'><%= image_tag('logout.png', :class => "log")%></a>
          <% else %>
          <a href='/login'><%= image_tag('login.png', :class => "log")%></a>
        <% end %>
      </div>
      <div class="debug fl-r p-r-sm">
        <% if current_user %>
          <a href='/mycities'><%= image_tag('heart.png', :class => "log")%></a>
          <% else %>
          <a href='/users/new' class="t-left w-60 h-68"><%= image_tag('plus.png', :class => "log")%></a>
        <% end %>
      </div>
      <div class="debug center w-logo">
        <a href="/cities"><%= image_tag('swipesee_circle.png', :class => "logo")%></a>
      </div>
    </div>
    <div class="t-center m-t-neg">
      <a href="/cities"><h1 class="f-oswald fz-ms c-white">SwipeSee</h1></a>
    </div>
    <div class="debug col span-12-t t-center p-sm">
      <form id="search-form" class="debug">
        <input type="text" class="debug bdr-blk t-center p-sm bdr-r-m w-90 fz-sm">
      </form>
    </div>
  </header>

  <div class="se-pre-con"></div>

  <div class="my-attraction-detail">
    <h1 class="debug"><%= @attraction.name %></h1>
    <div class="debug"><img id="swipe" src="<%= @attraction.photo_url %>"><div> 
      <a href="/mycities/<%=@city.id%>/attractions/<%=@attraction.id%>/maps" class="button-map" id='map' rel="external">
      Map me!</a> 
      <button href="" type='button' class="button-multi-map" id='bigmap'>Map me on big route!</button> </div>

    </div>
    <p id="swipe_status"></p>
    <div class="debug"><%= @attraction.description %></div>
  </div>
</div>