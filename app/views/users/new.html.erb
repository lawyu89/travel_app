<h1> Sign Up </h1>

<%= form_for @user do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name%>
  
  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :username %>
  <%= f.text_field :username %>


  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Sign Up" %>
<% end %>